   "source": [
    "def solution(nums):\n",
    "    answer = []\n",
    "    for x in nums:\n",
    "        if x not in answer:\n",
    "            answer.append(x)\n",
    "            \n",
    "    return min(len(nums)//2, len(answer))"
   ]
